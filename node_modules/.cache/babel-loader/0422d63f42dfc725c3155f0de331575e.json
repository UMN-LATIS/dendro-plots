,CAAC,EAiBR;AAhBA,MAAA,OAAK,EAAA,iBAAA,MAAA;AAAA,eAAE,QAAA,CAAA,KAAA,CAAM,MAAN,EAAc,GAAG,CAAC,EAAlB,EAAsB,GAAG,CAAC,MAA1B,CAAF;AAAA;AAgBL,KArBN,E,CAQE,YAAA,CAEE,eAFF,EAEE;AAFK,MAAA,IAAI,EAAE,GAAG,CAAC,IAEf;AADK,MAAA,IAAI,EAAE,GAAG,CAAC;AACf,KAFF,E,IAAA,E;;AAAA,M,gBAAA,C,EAIA,YAAA,CAEE,iBAFF,EAEE;AAFM,eAAM,QAEZ;AADO,MAAA,EAAE,EAAE,GAAG,CAAC;AACf,KAFF,E,IAAA,E;;AAAA,M,MAAA,C,EAOa,GAAG,CAAC,MAAJ,IAAU,CAAK,GAAG,CAAC,W,kBAHhC,YAAA,CAIE,gBAJF,EAIE;YAAA;AAJK,eAAM,OAIX;AAHM,MAAA,EAAE,EAAE,GAAG,CAAC,EAGd;AAFM,MAAA,GAAG,EAAE,GAAG,CAAC;AAEf,KAJF,E,IAAA,E;;AAAA,M,aAAA,C,sCAhBF,E;;AAAA,M,iBAAA,C;GAqBM,CArBN,E;;AAAA,G","sourcesContent":["<template>\r\n  <div class=\"data-entry-wrapper\"\r\n       v-for=\"obj in pairs\"\r\n       :key=\"obj.id\"\r\n       :class=\"{ active: obj.active }\"\r\n       :id=\"obj.id\"\r\n       @click=\"click($event, obj.id, obj.active)\"\r\n  >\r\n\r\n    <Name :name=\"obj.name\"\r\n          :file=\"obj.file\"\r\n    />\r\n\r\n    <Remove class=\"remove\"\r\n            :id=\"obj.id\"\r\n    />\r\n\r\n    <Modal class=\"modal\"\r\n           :id=\"obj.id\"\r\n           :top=\"obj.top\"\r\n           v-if=\"obj.active && !obj.outOfBounds\"\r\n    />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import Name from './SubComponents/Name.vue'\r\n  import Remove from './SubComponents/Remove.vue'\r\n  import Modal from './SubComponents/Modal.vue'\r\n\r\n  export default {\r\n    inject: ['store'],\r\n    props: ['useCache'],\r\n    components: { Name, Remove, Modal },\r\n    data() {\r\n      return {\r\n        states: (this.useCache) ? this.store.cache.states : this.store.states.current\r\n      }\r\n    },\r\n    computed: {\r\n      pairs: function() {\r\n        // combine state & cache info so it can be used as props\r\n        return this.states.map(stateObj => {\r\n          let modalObj = this.store.cache.modals.find(o => o.id == stateObj.id)\r\n          let pairObj = new Object()\r\n          pairObj.id = stateObj.id\r\n          pairObj.name = stateObj.name\r\n          pairObj.file = stateObj.file\r\n          pairObj.active = modalObj.active\r\n          pairObj.top = modalObj.top\r\n          pairObj.outOfBounds = modalObj.outOfBounds\r\n          return pairObj\r\n        })\r\n      }\r\n    },\r\n    methods: {\r\n      click: function(e, id, active) {\r\n        if (e.target.closest('.remove') || e.target.closest('.modal')) {\r\n          return\r\n        }\r\n        this.store.methods.updateCache('modals', id, 'active', !active)\r\n      },\r\n      onScroll: function() {\r\n        let dataEntries = document.getElementsByClassName('data-entry-wrapper')\r\n        for (const e of dataEntries) {\r\n          let modalObj = this.store.cache.modals.find(o => o.id == e.id)\r\n          if (e.getBoundingClientRect().top > 54) {\r\n            modalObj.top = e.getBoundingClientRect().top - 1\r\n            modalObj.outOfBounds = false\r\n          } else {\r\n            modalObj.outOfBounds = true\r\n          }\r\n        }\r\n      },\r\n    },\r\n    updated() {\r\n      this.onScroll()\r\n\r\n      document.getElementById('data-management').addEventListener('scroll', this.onScroll)\r\n    },\r\n    beforeDestroy() {\r\n      document.getElementById('data-management').removeEventListener('scroll', this.onScroll)\r\n    },\r\n  }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  .data-entry-wrapper {\r\n    width: 100%;\r\n    height: 20px;\r\n    padding: 0;\r\n    padding-left: 5px;\r\n    margin: 0;\r\n  }\r\n\r\n  .data-entry-wrapper:hover {\r\n    background-color: #d9d9d9;\r\n  }\r\n\r\n  .active {\r\n    background-color: #bdbdbd !important;\r\n  }\r\n\r\n  .remove {\r\n    float: right;\r\n  }\r\n\r\n</style>\r\n"],"sourceRoot":""}]}