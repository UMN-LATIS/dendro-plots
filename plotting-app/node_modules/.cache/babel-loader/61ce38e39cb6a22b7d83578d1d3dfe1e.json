ted=\"!showDefault\" disabled hidden> View Files </option>\r\n      <option v-for=\"name in fileNames\" :key=\"name\" :value=\"name\"> {{ name }} </option>\r\n    </select>\r\n    <button type=\"button\" id=\"file-delete\" @click=\"removeFile\">Remove File</button>\r\n  </div>\r\n  <div style=\"display: block;\">\r\n    <button type=\"button\" id=\"load-files\" @click=\"emitToParent\">Load Files</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  export default {\r\n    emits: ['fileLoad'],\r\n    data() {\r\n      return {\r\n        files: [],\r\n        fileNames: [],\r\n      }\r\n    },\r\n    computed: {\r\n      showDefault: function () {\r\n        return (this.fileNames.length) ? true : false\r\n      },\r\n    },\r\n    methods: {\r\n      uploadFiles() {\r\n        for (let file of this.$refs.fileInput.files) {\r\n          if (this.fileNames.includes(file.name) == false) {\r\n            this.files.push(file)\r\n            this.fileNames.push(file.name)\r\n          }\r\n        }\r\n      },\r\n      removeFile() {\r\n        let fileIndex = this.fileNames.indexOf(this.$refs.fileSelect.value)\r\n        this.files.splice(fileIndex, 1)\r\n        this.fileNames.splice(fileIndex, 1)\r\n      },\r\n      emitToParent() {\r\n        this.$emit('fileLoad', this.files)\r\n      },\r\n    }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n  h2 {\r\n    font-family: Sans-serif;\r\n    color: #797979;\r\n    margin: 0 10px;\r\n  }\r\n\r\n  p {\r\n    font-family: Sans-serif;\r\n    font-weight: bold;\r\n    color: #797979;\r\n    margin: 0 0 10px 10px;\r\n  }\r\n\r\n  label, select, button {\r\n    background-color: #797979;\r\n    color: white;\r\n    width: 120px;\r\n    padding: 4px 6px;\r\n    font-family: sans-serif;\r\n    font-size: 14px;\r\n    font-weight: bold;\r\n    border: none;\r\n    border-radius: 4px;\r\n    cursor: pointer;\r\n    margin: 10px 10px -5px 10px;\r\n  }\r\n\r\n  label {\r\n    padding: 5px 88px;\r\n  }\r\n\r\n  button {\r\n    border: 1px solid #797979;\r\n  }\r\n\r\n  label:hover, button:hover {\r\n    background-color: #f6f6f6;\r\n    color: #797979;\r\n    border: 1px solid #797979;\r\n  }\r\n\r\n  label:hover {\r\n    padding: 4px 87px;\r\n  }\r\n\r\n</style>\r\n"],"sourceRoot":""}]}